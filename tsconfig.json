{
  "compilerOptions": {
    "target": "es6",
    "module": "CommonJS",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "baseUrl": "./src",
    "paths": {
      "*": ["*"]
    },
    "outDir": "./dist",
    "rootDir": "./src",
    "noImplicitAny": false,
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}